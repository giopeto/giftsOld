<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Gifts</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.gifts.login.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SetGiftPicturesServlet</servlet-name>
    <servlet-class>com.gifts.setGifts.SetGiftPicturesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetGiftPicturesServlet</servlet-name>
    <url-pattern>/SetGiftPicturesServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SetGiftsInfoServlet</servlet-name>
    <servlet-class>com.gifts.setGifts.SetGiftsInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetGiftsInfoServlet</servlet-name>
    <url-pattern>/SetGiftsInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewGiftsController</servlet-name>
    <servlet-class>com.gifts.view.ViewGiftsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewGiftsController</servlet-name>
    <url-pattern>/ViewGiftsController</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.gifts.user.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>